<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<!-- For copyright / license terms, see COPYRIGHT.rst (top level of repository)
     Repository: https://github.com/C3S/collecting_society -->

<html xmlns="http://www.w3.org/1999/xhtml" i18n:domain="collecting_society_portal">

    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <title>${context.registry['meta']['title']|''}</title>
        <meta name="keywords" content="${context.registry['meta']['keywords']|''}">
        <meta name="description" content="${context.registry['meta']['description']|''}" />

        <link href="${request.static_path('deform:static/css/bootstrap.min.css')}"
              type="text/css" rel="stylesheet" media="screen" charset="utf-8" />
        <link href="${request.static_path('deform:static/css/form.css')}"
              type="text/css" rel="stylesheet" />
        <link href="${request.static_path('deform:static/select2/select2.css')}"
              type="text/css" rel="stylesheet" />
        <link href="${request.static_path('deform:static/pickadate/themes/default.css')}"
              type="text/css" rel="stylesheet" />
        <link href="${request.static_path('deform:static/pickadate/themes/default.date.css')}"
              type="text/css" rel="stylesheet" />
        <link href="${request.static_path('deform:static/pickadate/themes/default.time.css')}"
              type="text/css" rel="stylesheet" />

        <link href="${request.static_path('collecting_society_portal:static/img/favicon.ico')}" rel="icon" />
        <link href="${request.static_path('collecting_society_portal:static/css/base.css')}"
              type="text/css" rel="stylesheet" media="screen" charset="utf-8" />

        <!-- slot: css -->
        <tal:block metal:define-slot="css" />

        <link tal:condition="context.registry['static']['css']|False"
              tal:repeat="css context.registry['static']['css']"
              tal:attributes="href css"
              type="text/css" rel="stylesheet" media="screen" charset="utf-8" />

        <script src="${request.static_path('deform:static/scripts/jquery-2.0.3.min.js')}"></script>

        <script src="${request.static_path('deform:static/scripts/bootstrap.min.js')}"></script>
        <script src="${request.static_path('deform:static/scripts/jquery.form-3.09.js')}"></script>

        <script src="${request.static_path('deform:static/scripts/deform.js')}"></script>
        <script src="${request.static_path('deform:static/select2/select2.js')}"></script>

        <script src="${request.static_path('deform:static/pickadate/legacy.js')}"></script>
        <script src="${request.static_path('deform:static/pickadate/picker.js')}"></script>
        <script src="${request.static_path('deform:static/pickadate/picker.date.js')}"></script>
        <script src="${request.static_path('deform:static/pickadate/picker.time.js')}"></script>

        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

        <!-- slot: js head -->
        <tal:block metal:define-slot="js-head" />

    </head>

    <body>

        <!-- slot: content -->
        <tal:block metal:define-slot="content" />

        <!-- slot: js body -->
        <tal:block metal:define-slot="js-body" />

    </body>

</html>
